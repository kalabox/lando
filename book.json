{
  "title": "Lando Documentation",
  "root": "./docs",
  "gitbook": "3.2.2",
  "language": "en",
  "lunr": {
    "maxIndexSize": 1000000000
  },
  "structure": {
    "readme":  "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "anchors",
    "callouts",
    "codesnippet",
    "edit-link",
    "highlight",
    "gtm",
    "toc",
    "-sharing"
  ],
  "pluginsConfig": {
    "callouts": {
      "showTypeInHeader": false
    },
    "edit-link": {
      "base": "https://github.com/lando/lando/edit/master/docs",
      "label": "Edit Page"
    },
    "gtm": {
      "token": "GTM-TKLWK67",
      "virtualPageViews": true
    }
  }
}
